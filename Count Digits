import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int t = scn.nextInt();
        int count=0;

        while(n>0){
            int rem = n%10;
            n=n/10;
            if( rem == t){
                count++;
            }
        }
        System.out.println(count);

    }
}
